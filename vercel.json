{
  "version": 2,
  "functions": {
    "server/src/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/src/index.ts"
    },
    {
      "source": "/sync/:path*",
      "destination": "/server/src/index.ts"
    }
  ]
}
